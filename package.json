{
  "name": "package",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "build-storybook": "storybook build",
    "dev": "vite",
    "gitea_api:build": "npx @openapitools/openapi-generator-cli generate -i ./src/generated/gitea/definition/swagger.json --type-mappings AnyType=any,UNKNOWN_BASE_TYPE=object,enumPropertyNaming=original  -g typescript-axios -o ./src/generated/gitea --additional-properties=withInterfaces=true,enumPropertyNaming=original",
    "gitea_api:get": "curl https://try.gitea.io/swagger.v1.json --output ./src/generated/gitea/definition/swagger.json",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test.axe": "npx nightwatch test/E2E/axe.js --env chrome",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
